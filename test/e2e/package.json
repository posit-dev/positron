{
  "name": "positron-e2e-tests",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "compile": "npm run --prefix ../automation compile && node ../../node_modules/typescript/bin/tsc",
    "watch-automation": "npm run --prefix ../automation watch",
    "watch-e2e": "node ../../node_modules/typescript/bin/tsc --watch --preserveWatchOutput",
    "watch": "npm-run-all -lp watch-automation watch-e2e"
  },
  "dependencies": {
    "canvas": "^2.11.2",
    "mkdirp": "^1.0.4",
    "ncp": "^2.0.0",
    "node-fetch": "^2.6.7",
    "resemblejs": "5.0.0",
    "rimraf": "3.0.2"
  },
  "devDependencies": {
    "@types/mkdirp": "^1.0.1",
    "@types/ncp": "2.0.1",
    "@types/node": "20.x",
    "@types/node-fetch": "^2.5.10",
    "@types/resemblejs": "4.1.3",
    "@types/rimraf": "3.0.2",
    "archiver": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "watch": "^1.0.2"
  }
}
