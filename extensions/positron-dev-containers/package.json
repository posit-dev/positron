					"command": "remote-containers.createDevContainerFile",
					"group": "remote_30_dev-container_2_actions@4",
					"when": "workspaceFolderCount != 0 && remoteName =~ /^(wsl|ssh-remote)$/"
				},
				{
					"command": "remote-containers.openRepositoryInUniqueVolume",
