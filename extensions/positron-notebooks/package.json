{
  "name": "positron-notebooks-helpers",
  "displayName": "Positron Notebooks Helpers",
  "description": "Positron Notebook Helpers",
  "version": "1.0.0",
  "publisher": "positron",
  "icon": "icon.png",
  "private": true,
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "positronNotebookHelpers.convertImageToBase64",
        "title": "Convert Image to Base64"
      },
      {
        "command": "positronNotebookHelpers.fetchRemoteImage",
        "title": "Fetch Remote Image"
      },
      {
        "command": "positronNotebookHelpers.walkthrough.enableNotebook",
        "title": "Enable Positron Notebook"
      }
    ],
    "walkthroughs": [
      {
        "id": "positron.notebooks.welcome",
        "title": "%walkthrough.notebooks.welcome.title%",
        "description": "%walkthrough.notebooks.welcome.description%",
        "steps": [
          {
            "id": "positron.notebooks.panesAndUI",
            "title": "%walkthrough.notebooks.panesAndUI.title%",
            "description": "%walkthrough.notebooks.panesAndUI.description%",
            "media": {
              "markdown": "resources/walkthroughs/panes-and-ui.md"
            },
            "completionEvents": [
              "onCommand:workbench.action.customizeLayout"
            ]
          },
          {
            "id": "positron.notebooks.interpreter",
            "title": "%walkthrough.notebooks.interpreter.title%",
            "description": "%walkthrough.notebooks.interpreter.description%",
            "media": {
              "markdown": "resources/walkthroughs/interpreter.md"
            },
            "completionEvents": [
              "onCommand:workbench.action.language.runtime.selectSession"
            ]
          },
          {
            "id": "positron.notebooks.aiAssistant",
            "title": "%walkthrough.notebooks.aiAssistant.title%",
            "description": "%walkthrough.notebooks.aiAssistant.description%",
            "media": {
              "markdown": "resources/walkthroughs/ai-assistant.md"
            },
            "completionEvents": [
              "onCommand:workbench.action.chat.open"
            ]
          },
          {
            "id": "positron.notebooks.optIn",
            "title": "%walkthrough.notebooks.optIn.title%",
            "description": "%walkthrough.notebooks.optIn.description%",
            "media": {
              "markdown": "resources/walkthroughs/opt-in.md"
            },
            "completionEvents": [
              "onSettingChanged:positron.notebook.enabled"
            ]
          }
        ]
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vscode/test-electron": "^2.1.2",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.5",
    "@vscode/vsce": "^3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/posit-dev/positron"
  }
}
