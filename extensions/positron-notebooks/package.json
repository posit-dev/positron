    "@typescript-eslint/parser": "^5.12.1",
    "@vscode/test-electron": "^2.1.2",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.5",
    "vsce": "^2.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/posit-dev/positron"
