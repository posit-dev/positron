{
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"outDir": "out",
		"lib": ["ES2022"],
		"sourceMap": true,
		"rootDir": "src",
		"noFallthroughCasesInSwitch": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"esModuleInterop": true
	},
	"include": [
		"src/**/*",
		"../../src/vscode-dts/vscode.d.ts",
		"../../src/positron-dts/positron.d.ts"
	],
}
